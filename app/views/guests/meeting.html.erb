    
<style>
.bbottom{
  border-bottom:2px solid grey;
}
</style> 

<div class="container">
    <div style="width=100%;height:55px" class="bbottom mb-3">
      <h1 >
        Schedule Meeting 
      </h1>
</div>

<div class="container">
<div class="jumbotron jumbotron-fluid py-4 px-3">
    <div class="container">
    <%= form_tag meeting_guests_path, :method => 'get' do %>
      <%= label_tag :Phone%>:
      <%= text_field_tag :search,params[:search]%>
      <%= submit_tag "search" %>
    <% end %> 
  <br>
  <div class="px-4">
  <% if !@guestfind.nil? %>     
    <%= render 'editform', visitor: @guestfind %>
  <% else %>
    <div id="new-visitor">
    <%= render 'newform', visitor: @guestnew %>
    </div>
  <% end %>
  </div>
  </div>
  </div>
  </div>
  </div>