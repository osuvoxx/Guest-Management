    
<style>
.bbottom{
  border-bottom:2px solid grey;
}
</style> 
  
<div style="width=100%;height:55px" class="bbottom">
        <h1>Schedule Meeting </h1>
</div>
<br>
  <div class="container">
    <%= form_tag meeting_guests_path, :method => 'get' do %>
      <%= label_tag :Phone%>:
      <%= text_field_tag :search,params[:search]%>
      <%= submit_tag "search" %>
    <% end %> 
  </div>

  <% if !@guestfind.nil? %>     
    <%= render 'editform', visitor: @guestfind %>
  <% else %>
    <div id="new-visitor">
    <%= render 'newform', visitor: @guestnew %>
    </div>
  <% end %>


